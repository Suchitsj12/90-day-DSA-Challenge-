1) hashing 

#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;

    int arr[n];
    for (int i=0;i<n;i++){
        cin>>arr[i];
    }

    int hash[13] = {0};

    for (int i = 0; i < n; i++) {  // Use < instead of <=
        // cin >> arr[i];
        hash[arr[i]] += 1;         // Use arr[i] as an index to update hash
    }

    int q;
    cin >> q;

    while (q--) {
        int number;
        cin >> number;
        cout << hash[number] << endl;
    }

    return 0;
}



